{
  "name": "ngneat-inspector",
  "description": "A chrome extension to inspect and change Angular component properties, by ngneat.",
  "version": "0.1.0",
  "scripts": {
    "build": "gulp build",
    "changelog:current": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:start": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "precommit": "npm run lint",
    "commit": "npx git-cz",
    "lint": "run-s eslint prettier",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "eslint": "npx eslint src/**/*.ts",
    "eslint:fix": "npx eslint src/**/*.ts --fix",
    "prettier": "npx prettier src/**/*.{ts,json,html} --check",
    "prettier:fix": "npx prettier src/**/*.{ts,json,html} --write",
    "ejslint": "npx ejslint src/inject/html-generators/index.ejs",
    "start": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dharmen Shah",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.121",
    "@types/ejs": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "all-contributors-cli": "^6.17.2",
    "browserify": "^16.5.1",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.2.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.9.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "tsify": "^4.0.2",
    "typescript": "^3.9.7",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/prismjs": "^1.16.1",
    "bootstrap": "^4.5.1",
    "clipboard": "^2.0.6",
    "ejs": "^3.1.3",
    "prismjs": "^1.21.0",
    "tippy.js": "^6.2.5"
  }
}
