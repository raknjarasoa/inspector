{
  "name": "chrome-ext-ng-props",
  "version": "1.0.0",
  "description": "A chrome extension to inspect and change Angular component properties.",
  "scripts": {
    "build": "gulp main",
    "changelog:current": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:start": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "npx git-cz",
    "start": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Dharmen Shah",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "0.0.121",
    "browserify": "^16.5.1",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.2.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "tippy.js": "^6.2.5",
    "tsify": "^4.0.2",
    "typescript": "^3.9.7",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
